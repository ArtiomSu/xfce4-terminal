#: ../terminal/main.c:42
msgid "Usage: Terminal [OPTION...]"
msgstr "Användning: Terminal [INSTÄLLNING...]"

#: ../terminal/main.c:44
msgid "  -h, --help                          Print this help message and exit"
msgstr ""
"  -h, --help                          Skriv det här meddelandet och avsluta"

#: ../terminal/main.c:45
msgid ""
"  -v, --version                       Print version information and exit"
msgstr ""
"  -v, --version                       Skriv versionsinformationen och avsluta"

#: ../terminal/main.c:46
msgid ""
"  --disable-server                    Do not register with the D-BUS\n"
"                                      session message bus"
msgstr ""
"  --disable-server                    Registrera inte mot D-BUS sessions-\n"
"                                      meddelandebusen"

#: ../terminal/main.c:49
msgid ""
"  -x, --execute                       Execute the remainder of the command\n"
"                                      line inside the terminal"
msgstr ""
"  -x, --execute                       Kör återstoden av kommandoraden inuti\n"
"                                      terminalen."

#: ../terminal/main.c:51
msgid ""
"  -e, --command=STRING                Execute the argument to this option\n"
"                                      inside the terminal"
msgstr ""
"  -e, --command=STRÄNG                Kör argumentet till den här "
"kommandorads-\n"
"                                      inställningen inuti terminalen."

#: ../terminal/main.c:53
msgid ""
"  --working-directory=DIRNAME         Set the terminals working directory"
msgstr "  --working-directory=KATALOGNAMN     Sätt terminalens arbetskatalog"

#: ../terminal/main.c:54
msgid "  -T, --title=TITLE                   Set the terminals title"
msgstr "  -T, --title=TITEL                   Sätt terminalens fönstertitel"

#: ../terminal/main.c:55
msgid ""
"  -H, --hold                          Do not immediately destroy the tab\n"
"                                      when the child command exits"
msgstr ""
"  -H, --hold                          Stäng inte tabben då kommandot är "
"färdigt"

#: ../terminal/main.c:58
msgid ""
"  --display=DISPLAY                   X display to use for the last-\n"
"                                      specified window"
msgstr ""
" --display=DISPLAY                    X-display som ska användas för det\n"
"                                      senaste fönstret"

#: ../terminal/main.c:60
msgid ""
"  --geometry=GEOMETRY                 X geometry specification (see \"X\"\n"
"                                      man page), can be specified once per\n"
"                                      window to be opened"
msgstr ""
"  --geometry=GEOMETRI                 X-geometriinställning (se manpagen "
"för\n"
"                                      \"X\"), kan anges en gång per fönster\n"
"                                      som ska öppnas"

#: ../terminal/main.c:63
msgid ""
"  --role=ROLE                         Set the role for the last-specified;\n"
"                                      window; applies to only one window;\n"
"                                      can be specified once for each window\n"
"                                      you create from the command line"
msgstr ""
"  --role=ROLL                         Sätt rollen för det senaste fönstret;\n"
"                                      gäller bara för ett fönster; kan "
"anges\n"
"                                      en gång per fönster som du skapar "
"från\n"
"                                      kommandoraden"

#: ../terminal/main.c:67
msgid ""
"  --startup-id=STRING                 ID for the startup notification\n"
"                                      protocol"
msgstr ""
" --startup-id=STRÄNG                  ID för uppstartsnotifieringsprotokollet"

#: ../terminal/main.c:69
msgid ""
"  --fullscreen                        Set the last-specified window into\n"
"                                      fullscreen mode; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line."
msgstr ""
"  --fullscreen                        Sätter det senaste fönstret till att\n"
"                                      köras i fullskärm; gäller bara för\n"
"                                      ett fönster; kan anges en gång för "
"varje\n"
"                                      fönster som du skapar från "
"kommandoraden."

#: ../terminal/main.c:74
msgid ""
"  --show-menubar                      Turn on the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-menubar                      Aktivera menyraden för det senaste\n"
"                                      fönstret; gäller bara för ett "
"fönster;\n"
"                                      kan anges en gång för varje fönster "
"du\n"
"                                      skapar från kommandoraden"

#: ../terminal/main.c:79
msgid ""
"  --hide-menubar                      Turn off the menubar for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-menubar                      Stäng av menyraden för det senaste\n"
"                                      fönstret; gäller bara för ett "
"fönster;\n"
"                                      kan anges en gång för varje fönster "
"du\n"
"                                      skapar från kommandoraden"

#: ../terminal/main.c:84
msgid ""
"  --show-borders                      Turn on the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --show-borders                      Aktivera fönsterdekorationerna för "
"det\n"
"                                      senaste fönstret; gäller bara för ett\n"
"                                      fönster; kan anges en gång för varje\n"
"                                      fönster du skapar från kommandoraden"

#: ../terminal/main.c:89
msgid ""
"  --hide-borders                      Turn off the window decorations for\n"
"                                      the last-specified window; applies\n"
"                                      to only one window; can be specified\n"
"                                      once for each window you create from\n"
"                                      the command line"
msgstr ""
"  --hide-borders                      Stäng av fönsterdekorationerna för "
"det\n"
"                                      senaste fönstret; gäller bara för ett\n"
"                                      fönster; kan anges en gång för varje\n"
"                                      fönster du skapar från kommandoraden"

#: ../terminal/main.c:94
msgid ""
"  --show-toolbars                     Turn on the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --show-toolbars                     Aktivera av verktygsraden för det\n"
"                                      senaste fönstret; gäller bara för ett\n"
"                                      fönster; kan anges en gång för varje\n"
"                                      fönster du skapar från kommandoraden"

#: ../terminal/main.c:99
msgid ""
"  --hide-toolbars                     Turn off the toolbars for the last-\n"
"                                      specified window; applies to only one\n"
"                                      window; can be specified once for\n"
"                                      each window you create from the\n"
"                                      command line"
msgstr ""
"  --hide-toolbars                     Stäng av verktygsraden för det\n"
"                                      senaste fönstret; gäller bara för ett\n"
"                                      fönster; kan anges en gång för varje\n"
"                                      fönster du skapar från kommandoraden"

#: ../terminal/main.c:105
msgid ""
"  --tab                               Open a new tab in the last-specified\n"
"                                      window; more than one of these\n"
"                                      options can be provided"
msgstr ""
" --tab                                Öppna en ny tabb i det senaste "
"fönstret;\n"
"                                      mer än en av det här "
"kommandoradsargumentet\n"
"                                      kan anges"

#: ../terminal/main.c:108
msgid ""
"  --window                            Open a new window containing one tab;\n"
"                                      more than one of these options can be\n"
"                                      provided"
msgstr ""
" --window                             Öppna ett nytt fönster som innehåller "
"en tabb;\n"
"                                      mer än en av det här "
"kommandoradsargumentet\n"
"                                      kan anges"

#: ../terminal/main.c:112
msgid "  --default-display=DISPLAY           default X display to use"
msgstr ""
"  --default-display=DISPLAY          X-display att använda som standard"

#: ../terminal/main.c:113
msgid ""
"  --default-working-directory=DIRNAME Set the default terminals working\n"
"                                      directory"
msgstr ""
"  --default-working-directory=DIRNAME Sätt terminalens arbetskatalog som\n"
"                                      den ska använda som standard"

