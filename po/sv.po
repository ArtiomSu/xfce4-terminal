msgid "_File:"
msgstr "_Fil:"

#: ../terminal/terminal-preferences-dialog.c:512
msgid "_Style:"
msgstr "_Stil:"

#: ../terminal/terminal-preferences-dialog.c:519
msgid "Tiled"
msgstr "Sida vid sida"

#: ../terminal/terminal-preferences-dialog.c:520
msgid "Centered"
msgstr "Centrerad"

#: ../terminal/terminal-preferences-dialog.c:521
msgid "Scaled"
msgstr "Skalad"

#: ../terminal/terminal-preferences-dialog.c:522
msgid "Stretched"
msgstr "Sträckt"

#: ../terminal/terminal-preferences-dialog.c:538
msgid "Shade transparent or image background:"
msgstr "Skugga genomskinlighet eller bakgrund:"

#: ../terminal/terminal-preferences-dialog.c:551
msgid "<small><i>None</i></small>"
msgstr "<small><i>Inget</i></small>"

#: ../terminal/terminal-preferences-dialog.c:562
msgid "<small><i>Maximum</i></small>"
msgstr "<small><i>Maximalt</i></small>"

#: ../terminal/terminal-preferences-dialog.c:570
msgid "<b>Opening New Windows</b>"
msgstr "<b>Nya fönster</b>"

#: ../terminal/terminal-preferences-dialog.c:579
msgid "Display _menubar in new windows"
msgstr "Visa _menyrad i nya fönster"

#: ../terminal/terminal-preferences-dialog.c:582
msgid ""
"Enable this option to show the menubar in newly created terminal windows."
msgstr ""
"Aktivera detta alternativ för att visa menyraden i nyöppnade terminalfönster."

#: ../terminal/terminal-preferences-dialog.c:587
msgid "Display _toolbars in new windows"
msgstr "Visa _verktygsfält i nya fönster"

#: ../terminal/terminal-preferences-dialog.c:590
msgid ""
"Enable this option to show the toolbars in newly created terminal windows."
msgstr ""
"Aktivera detta alternativ för att visa verktygsfältet i nyöppnade "
"terminalfönster."

#: ../terminal/terminal-preferences-dialog.c:595
msgid "Display _borders around new windows"
msgstr "Visa _kanter runt nya fönster"

#: ../terminal/terminal-preferences-dialog.c:598
msgid ""
"Enable this option to show window decorations around newly created terminal "
